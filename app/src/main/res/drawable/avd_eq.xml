<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:width="30dp"
            android:height="20dp"
            android:viewportWidth="30"
            android:viewportHeight="20"
            android:tint="?colorTertiary">
            <group
                android:name="group"
                android:pivotY="20">
                <path
                    android:name="path"
                    android:pathData="M 0 0 L 5 0 L 5 20 L 0 20 Z"
                    android:fillColor="#fff"/>
            </group>
            <group
                android:name="group_1"
                android:pivotY="20"
                android:translateX="6">
                <path
                    android:name="path_1"
                    android:pathData="M 0 0 L 5 0 L 5 20 L 0 20 Z"
                    android:fillColor="#fff"/>
            </group>
            <group
                android:name="group_2"
                android:pivotY="20"
                android:translateX="12">
                <path
                    android:name="path_2"
                    android:pathData="M 0 0 L 5 0 L 5 20 L 0 20 Z"
                    android:fillColor="#fff"/>
            </group>
            <group
                android:name="group_3"
                android:pivotY="20"
                android:translateX="18">
                <path
                    android:name="path_3"
                    android:pathData="M 0 0 L 5 0 L 5 20 L 0 20 Z"
                    android:fillColor="#fff"/>
            </group>
            <group
                android:name="group_4"
                android:pivotY="20"
                android:translateX="24">
                <path
                    android:name="path_4"
                    android:pathData="M 0 0 L 5 0 L 5 20 L 0 20 Z"
                    android:fillColor="#fff"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="1500"
                    android:valueFrom="0.7"
                    android:valueTo="0.8"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="1500"
                    android:duration="500"
                    android:valueFrom="0.8"
                    android:valueTo="0.7"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="1000"
                    android:valueFrom="0.5"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="1000"
                    android:duration="1000"
                    android:valueFrom="1"
                    android:valueTo="0.5"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_2">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="1500"
                    android:valueFrom="0.3"
                    android:valueTo="0.8"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="1500"
                    android:duration="500"
                    android:valueFrom="0.8"
                    android:valueTo="0.3"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_3">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="500"
                    android:valueFrom="0.3"
                    android:valueTo="0.5"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="500"
                    android:duration="1500"
                    android:valueFrom="0.5"
                    android:valueTo="0.3"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_4">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="500"
                    android:valueFrom="0.6"
                    android:valueTo="0.2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="500"
                    android:duration="1500"
                    android:valueFrom="0.2"
                    android:valueTo="0.6"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
